// CONTADOR COM FOR
console.log("=== CONTADOR COM FOR ===");
for (let i = 1; i <= 10; i++) {
    console.log(i);
}

// CONTADOR COM WHILE
console.log("=== CONTADOR COM WHILE ===");
let contador = 1;
while (contador <= 10) {
    console.log(contador);
    contador++;
}

// CONTADOR COM DO...WHILE
console.log("=== CONTADOR COM DO...WHILE ===");
let contador2 = 1;
do {
    console.log(contador2);
    contador2++;
} while (contador2 <= 10);